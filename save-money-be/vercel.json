{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ],
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "crons": [
      {
        "path": "/api/cron/daily-scrape",
        "schedule": "0 6 * * *"
      },
      {
        "path": "/api/cron/daily-scrape",
        "schedule": "0 7 * * *"
      }
    ]
}